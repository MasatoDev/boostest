#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules/@vercel/nft/out/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules/@vercel/nft/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules/@vercel/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules/@vercel/nft/out/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules/@vercel/nft/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules/@vercel/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/@vercel+nft@0.26.5/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vercel+nft@0.26.5/node_modules/@vercel/nft/out/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@vercel+nft@0.26.5/node_modules/@vercel/nft/out/cli.js" "$@"
fi
