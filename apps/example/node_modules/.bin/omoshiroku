#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/invest/omo/packages/omoshiroku/node_modules:/home/ubuntu/invest/omo/packages/node_modules:/home/ubuntu/invest/omo/node_modules:/home/ubuntu/invest/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/invest/omo/packages/omoshiroku/node_modules:/home/ubuntu/invest/omo/packages/node_modules:/home/ubuntu/invest/omo/node_modules:/home/ubuntu/invest/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/invest/omo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../omoshiroku/index.js" "$@"
else
  exec node  "$basedir/../omoshiroku/index.js" "$@"
fi
