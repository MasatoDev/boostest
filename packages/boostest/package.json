{
  "name": "boostest",
  "version": "0.1.1",
  "description": "Boostest is a tool that generates test code from type definitions.",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "@MasatoDev:registry": "https://npm.pkg.github.com"
  },
  "bin": {
    "boostest": "./dist/cli.js"
  },
  "napi": {
    "name": "boostest",
    "triples": {}
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.3",
    "ava": "^6.0.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version",
    "type-check": "tsc --noEmit",
    "bin:build": "tsc"
  },
  "dependencies": {
    "@types/node": "^20.14.8",
    "@types/yargs": "^17.0.32",
    "build": "^0.1.4",
    "typescript": "^5.4.5",
    "yargs": "^17.7.2"
  },
  "optionalDependencies": {
    "boostest-win32-x64-msvc": "0.1.1",
    "boostest-darwin-x64": "0.1.1",
    "boostest-linux-x64-gnu": "0.1.1"
  },
  "author": "MasatoDev",
  "license": "MIT"
}
