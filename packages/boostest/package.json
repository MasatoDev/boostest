{
  "name": "@boostest/cli",
  "version": "0.7.0",
  "description": "Boostest is a tool that generates test code from type definitions.",
  "main": "index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bin": {
    "boostest": "./dist/cli.js"
  },
  "napi": {
    "name": "boostest",
    "triples": {
      "additional": [
        "darwin-arm64"
      ],
      "package": {
        "name": "boostest"
      }
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "ava": "^6.0.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js-package-name @boostest/boostest",
    "build:debug": "napi build --platform",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version",
    "type-check": "tsc --noEmit",
    "bin:build": "tsc"
  },
  "dependencies": {
    "@types/node": "^20.14.8",
    "@types/yargs": "^17.0.32",
    "build": "^0.1.4",
    "typescript": "^5.4.5",
    "yargs": "^17.7.2"
  },
  "optionalDependencies": {
    "@boostest/boostest-win32-x64-msvc": "0.7.0",
    "@boostest/boostest-darwin-x64": "0.7.0",
    "@boostest/boostest-linux-x64-gnu": "0.7.0",
    "@boostest/boostest-darwin-arm64": "0.7.0"
  },
  "author": "MasatoDev",
  "license": "MIT",
  "keywords": [
    "test",
    "test date",
    "mock",
    "code generator",
    "test framework",
    "test code",
    "command",
    "tool",
    "generator",
    "debug"
  ]
}
